class Complex:
    def __init__(self,real,imag):
        self.real=real
        self.imag=imag
    def __repr__(self):
        return "%s %s"%(self.real,self.imag)

    def __str__(self):
        return "%s %s"%(self.real,self.imag)
x=raw_input("enter value for real:",real)
y=raw_input("enter value for real:",imag)
t=Complex(x,y)
print repr(t)
print str(t)

        
