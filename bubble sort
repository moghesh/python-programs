def Bublesort(arr):
    n=len(arr)
    for i in range(0,n-1):
        flag=0
        for j in range(0,n-i-1):
            if arr[j]>arr[j+1]:
                arr[j],arr[j+1]=arr[j+1],arr[j]
                flag+=1
arr=[4, 34, 25, 12, 22, 11, 90]
Bublesort(arr)
print "buble sort"
for i in range(len(arr)):
    print ("%d"%arr[i]),
